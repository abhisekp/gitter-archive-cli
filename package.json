{
  "name": "gitter-archive-3",
  "version": "0.0.0",
  "description": "Gitter Chat room message downloader",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bin/cli.js",
    "start:dev": "cross-env NODE_ENV=development babel-node ./src/bin/cli.js",
    "prebuild": "rimraf bin/ lib/",
    "build": "babel -D -d . src",
    "build:watch": "babel -w -D -d . src",
    "test": "xo"
  },
  "bin": {
    "gitter-archive": "./bin/cli.js"
  },
  "author": "Abhisek Pattnaik <abhisekp@engineer.com> (https://about.me/abhisekp)",
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^2.0.1",
    "axios": "^0.15.3",
    "config": "^1.24.0",
    "cross-env": "^3.1.3",
    "csv-write-stream": "^2.0.0",
    "debug": "^2.3.3",
    "dotenv": "^2.0.0",
    "hjson": "^2.3.1",
    "lodash": "^4.17.2",
    "npm-run-all": "^3.1.2",
    "promise-retry": "^1.1.1",
    "rate-limiter-api": "^1.0.11",
    "rimraf": "^2.5.4",
    "stampit": "^3.0.6",
    "url-template": "^2.0.8",
    "winston": "^2.3.0",
    "winston-papertrail": "^1.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.0.2",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.1.0"
  },
  "babel": {
    "sourceMap": "inline",
    "presets": [
      "latest",
      "stage-0"
    ]
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node"
    ]
  }
}
